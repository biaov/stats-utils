{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm i",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.+)",
      "destination": "dist/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "dist/index.js"
    }
  ],
  "cleanUrls": true
}
