{
  "version": 2,
  "buildCommend": "npm run build",
  
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/csdn/(.*)",
      "destination": "https://blog.csdn.net/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "dist/index.js"
    },
    {
      "source": "/(.+)",
      "destination": "dist/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/biaov/stats-utils"
    }
  ],
  "cleanUrls": true
}
