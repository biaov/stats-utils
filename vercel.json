{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm i",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.+)",
      "destination": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/biaov/stats-utils"
    }
  ],
  "cleanUrls": true
}
