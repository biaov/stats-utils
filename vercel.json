{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm i",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "dist/index.js"
    },
    {
      "src": "/csdn/(.*)",
      "dest": "https://blog.csdn.net/$1"
    }
  ]
}
