{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm i",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/csdn/:match*",
      "destination": "https://blog.csdn.net/:match*"
    },
    {
      "source": "/api/(.*)",
      "destination": "dist/index.js"
    },
    {
      "source": "/(.+)",
      "destination": "dist/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/biaov/stats-utils"
    }
  ],
  "cleanUrls": true
}
