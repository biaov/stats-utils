{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm i",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.+)",
      "destination": "dist/index.js"
    }
  ],
  "functions": {
    "api/*": {
      "runtime": "@vercel/node@3.1.5"
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/biaov/stats-utils"
    }
  ],
  "cleanUrls": true
}
